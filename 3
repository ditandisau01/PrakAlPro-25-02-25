angkaPertama = int(input("Silahkan masukkan angka pertama\n>> "))
angkaKedua = int(input("Silahkan masukkan angka kedua\n>> "))
angkaKetiga = int(input("Silahkan masukkan angka ketiga\n>> "))

if angkaPertama > angkaKedua and angkaPertama > angkaKetiga:
    print("angka pertama paling besar")
elif angkaKedua > angkaPertama and angkaKedua > angkaKetiga:
    print("angka kedua paling besar")
elif angkaKetiga > angkaPertama and angkaKetiga > angkaKedua:
    print("angka ketiga paling besar")
else:
    print("ada angka yang sama")
